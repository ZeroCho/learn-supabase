{
  "name": "learn-supabase-course",
  "version": "1.0.0",
  "description": "Supabase 강의 예제 코드",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node --require dotenv/config",
    "build": "tsc",
    "start": "node --env-file .env dist/lib/supabase.js",
    "server": "ts-node --require dotenv/config src/server/auth-callback-server.ts",
    "server:build": "tsc && node --env-file .env dist/server/auth-callback-server.js",
    "gen:types": "npx supabase gen types typescript --project-id taijjwtaycfedswoyert > src/types/database.ts",
    "gen:types:local": "npx supabase gen types typescript --local > src/types/database.ts",
    "db:push": "npx supabase db push",
    "db:status": "npx supabase db remote commit"
  },
  "keywords": [
    "supabase",
    "typescript",
    "nodejs"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@types/express": "^5.0.6",
    "base64url": "^3.0.1",
    "dotenv": "^16.3.1",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
